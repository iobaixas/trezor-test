{
  "manifest_version": 2,
  "name": "Trezor Binding",
  "version": "1.0",
  "description": "This extension enables web3 with trezor",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["loader.js"]
    }
  ],
  "web_accessible_resources": [
    "connect.js",
    "binding.js"
  ]
}
